#include <iostream>
using namespace std;

int main(int argc, char* argv[])
{
    if (argc > 1) {
        string arg = argv[1];
        
        for (int i = 0; i < arg.length(); i++) {
            cout << arg << endl;
        }
    } else {
        cout << "No arguments provided." << endl;
    }

    return 0;
}
